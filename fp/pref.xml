	android:title="RING" />

<PreferenceCategory android:title="PROFILE">
	<EditTextPreference
		android:defaultValue="GUEST"
		android:key="edittext_name"
		android:summary="PROFILE"
		android:title="USER" />
	<ListPreference
		android:defaultValue="A"
		android:entries="@array/blood_items"
		android:entryValues="@array/blood_items"
		android:key="list"
		android:summary="SELECT"
		android:title="BLOOD_ITEMS" />
</PreferenceCategory>
<PreferenceCategory android:title="OPTIONS">
	<CheckBoxPreference
		android:defaultValue="true"
		android:key="chk"
		android:summary="CHECK"
		android:title="CHECK" />
</PreferenceCategory>













