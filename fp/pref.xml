//MainActivity8.java
<CheckBoxPreference
	android:defaultValue="true"
	android:key="chk"
	android:summary="CKECH ON"
	android:title="TITLE" />

<SwitchPrefrence
	android:defaultValue="true"
	android:key="switch"
	android:summary="CHECK ON"
	android:title="TITLE" />

<ListPreference
	android:defaultValue="A"
	android:entries="@array/blood_items"
	android:entryValues="@array/blood_items"
	android:key="list"
	android:summary="SELECT"
	android:title="BLOOD_ITEMS" />

<MultiSelectListPreference
	android:entries="@array/os_items"
	android:entryValues="@array/os_items"
